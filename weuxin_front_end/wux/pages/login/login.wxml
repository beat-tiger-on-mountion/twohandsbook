<view class='container'>

<view>
  <image src='{{head}}' class='headImg'></image>
</view>

<form>
<wux-select id="wux-select1" />
<view class="page">
  <view class="page__bd">
    <wux-cell title="学校" extra="{{ title1 }}" bind:click="onClick1"></wux-cell>
  </view>
</view>

<view class="page__bd">
  <wux-cell-group>
    <wux-cell hover-class="none">
      <wux-input label="账号" placeholder="输入账号" />
    </wux-cell>
    <wux-cell hover-class="none">
      <wux-input label="密码" placeholder="输入密码" password type="number" />
    </wux-cell>
  </wux-cell-group>
</view>

 <button class='but1' bindtap='login' > 登录 </button>
 <view class="sbuts">
  <button size='mini'>注册</button>
  <button size='mini'>忘记密码</button>
 </view>

</form>
</view>